{
  "extends": "@tsconfig/node16/tsconfig.json",
  "include": ["functions", "core"],
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@catan-discord/core/*": ["./core/*"],
      "@catan-discord/bot/*": ["./functions/bot/*"]
    }
  }
}
